---
import BaseForm from '@/contexts/_shared/client/presentation/components/forms/base-form.astro';

export interface Props {
    id?: string;
    dataset?: any;
    editMode?: boolean;
}

const {
    id,
    dataset,
    editMode,
} = Astro.props as Props;
---

<BaseForm id={id} class="flex justify-center" dataset={dataset} editMode={editMode}>
    <slot/>
</BaseForm>